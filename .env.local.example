# Example .env.local file for local development
# Copy this file to .env.local and customize as needed

# GitHub OAuth Credentials - replace with your own
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
NEXT_PUBLIC_GITHUB_CLIENT_ID=your-github-client-id

# API URLs
NEXT_PUBLIC_AUTH_URL=http://localhost
AUTH_PROXY_URL=http://localhost
NEXT_PUBLIC_API_URL=http://localhost

# WebSocket URLs
NEXT_PUBLIC_WS_URL=ws://localhost:80/ws

# Backend API URL
NEXT_PUBLIC_BACKEND_URL=http://localhost

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-super-secret-key-for-development-only

# OAuth Providers